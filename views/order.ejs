<%- include('header') -%>
<div class="colorlib-product" style="padding-top: 10px">
  <div style="padding-left: 10px; padding-right: 10px">
    <div class="row">
      <div class="col-md-4 offset-md-8 text-right">
        <div class="top-menu" style="padding-top: 10px">
          <form class="search-wrap">
            <div class="form-group">
              <input
                type="search"
                id="query-search"
                class="form-control search"
                placeholder="Search"
              />
              <button
                id="btn-search"
                class="btn btn-primary submit-search text-center"
                type="submit"
              >
                <i class="icon-search"></i>
              </button>
            </div>
          </form>
        </div>
      </div>
    </div>
    <div class="row">
      <div
        class="col-sm-8 offset-sm-2 text-center colorlib-heading"
        style="height: 0px"
      >
        <h2>Đơn hàng</h2>
      </div>
    </div>
    <div class="row mb-3">
      <div class="col-sm-12 text-left">
        <button id="add-category-btn" class="btn btn-primary">Thêm</button>
      </div>
    </div>
    <table
      class="table table-striped table-bordered table-hover table table-bordered"
    >
      <thead>
        <tr>
          <th scope="col">ID</th>
          <th scope="col">ID người dùng</th>
          <th scope="col">Số lượng sản phẩm</th>
          <th scope="col">Trạng thái</th>
          <th scope="col">Thời gian đặt hàng</th>
          <th scope="col"></th>
        </tr>
      </thead>
      <tbody id="order-table-body"></tbody>
    </table>
  </div>
</div>
<%- include('footer') -%>
